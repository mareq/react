{
  "name": "react-ts-babel-webpack_template",
  "version": "1.0.0",
  "description": "React & TypeScript & Babel & Webpack template</title>",
  "keywords": [
    "react",
    "typescript",
    "babel",
    "webpack",
    "template"
  ],
  "author": "Mareq Balint <mareq@balint.eu>",
  "license": "MIT",
  "scripts": {
    "start": "tsc --noEmit && webpack serve --mode=development",
    "build": "tsc --noEmit && webpack --mode=production",
    "clean": "rm -rf dist",
    "mrproper": "rm -rf node_modules .yarn.lock dist",
    "fmt": "prettier --check 'src/**/*.{js,jsx,ts,tsx,css,md,json}'",
    "fmt:fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}'",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx,json}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@eslint/create-config": "0.3.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-loader": "^8.2.5",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.7.1",
    "source-map-loader": "^4.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
